@page
@model IEnumerable<Social.Domain.Models.ServisesSocial>
@{
    int i = 0;
}
<table class="table table-hover">
    <thead>
        <th>
            №
        </th>
        <th>
            Номер заявления
        </th>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @i
                </td>
            </tr>
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DocNum)
                </td>
            </tr>
        }
    </tbody>
</table>
